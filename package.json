  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^16.14.35",
    "@types/react-dom": "^16.9.18",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@apollo/client": "^3.5.6",
